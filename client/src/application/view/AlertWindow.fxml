<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import de.jensd.fx.glyphs.fontawesome.*?>

<AnchorPane prefHeight="100.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.AlertWindowController">
   <children>
      <HBox alignment="CENTER_LEFT" nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0" prefWidth="400.0" style="-fx-background-color: #2c3e50;">
         <children>
            <FontAwesomeIconView fx:id="closeStageBtn" glyphName="TIMES_CIRCLE" onMouseClicked="#closeStage" size="16px" styleClass="topBarIcons" />
         </children>
         <padding>
            <Insets bottom="7.0" left="10.0" right="10.0" top="7.0" />
         </padding>
      </HBox>
      <VBox prefHeight="70.0" prefWidth="400.0" style="-fx-background-color: #efefef;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="400.0" spacing="20.0">
               <children>
                  <FontAwesomeIconView fx:id="alertIcon" glyphName="TIMES_CIRCLE" size="32px" />
                  <Label fx:id="message" maxWidth="1.7976931348623157E308" text="Label" wrapText="true" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
